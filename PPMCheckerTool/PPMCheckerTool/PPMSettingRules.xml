<PpmValidationRules version = "2023.1.0">
	
	<TargetCPU name="ADL_U">
		<Overlay name="Overlay_Default">

			<!--Default profile-->
			<Profile name="Profile_Default">

				<!--Setting EPP class0-->
				<Setting name="PERFEPP">
					
					<!--Min AC value-->
					<AcMinValue> 33 </AcMinValue>

					<!--Min DC value-->
					<DcMinValue> 60 </DcMinValue>

				</Setting>

				<!--Setting EPP class1-->
				<Setting name="PERFEPP1">
					
					<!--Min AC value-->
					<AcMinValue> 33 </AcMinValue>

					<!--Min DC value-->
					<DcMinValue> 60 </DcMinValue>

				</Setting>

				<!--Setting SoftParkLatency-->
				<Setting name="SOFTPARKLATENCY">

					<!-- Value = 0-->
					<DcValue> 0 </DcValue>
				</Setting>

				<!--Setting SmtUnparkPolicy-->
				<Setting name="SMTUNPARKPOLICY">

					<!-- Value = 3-->
					<DcValue> 3 </DcValue>
				</Setting>

				<!--Setting ModuleUnparkPolicy-->
				<Setting name="MODULEUNPARKPOLICY">

					<!-- Value = 2-->
					<DcValue> 2 </DcValue>

				</Setting>
			</Profile>

			<!--MedQoS profile-->
			<Profile name="Profile_MediumQos">

				<!--Setting EPP class0-->
				<Setting name="PERFEPP">

					<!--Distance to the default profile >= 20-->
					<DcMinDistanceToProfile>
						<Profile> Profile_Default </Profile>
						<Distance> 10 </Distance>
					</DcMinDistanceToProfile>

				</Setting>

				<!--Setting EPP class1-->
				<Setting name="PERFEPP1">

					<!--Distance to the default profile >= 20-->
					<DcMinDistanceToProfile>
						<Profile> Profile_Default </Profile>
						<Distance> 10 </Distance>
					</DcMinDistanceToProfile>

				</Setting>
			</Profile>

			<!--LowQoS profile-->
			<Profile name="Profile_LowQos">

				<!--Setting EPP class0-->
				<Setting name="PERFEPP">

					<!--Distance to the default profile >= 20-->
					<DcMinDistanceToProfile>
						<Profile> Profile_Default </Profile>
						<Distance> 10 </Distance>
					</DcMinDistanceToProfile>

				</Setting>

				<!--Setting EPP class1-->
				<Setting name="PERFEPP1">

					<!--Distance to the default profile >= 20-->
					<DcMinDistanceToProfile>
						<Profile> Profile_Default </Profile>
						<Distance> 10 </Distance>
					</DcMinDistanceToProfile>

				</Setting>

				<!--Frequency Cap-->
				<Setting name="PROCFREQMAX">

					<!--DC Fmax = 1500-->
					<DcMaxValue> 1500 </DcMaxValue>
				</Setting>

				<!--Frequency Cap1-->
				<Setting name="PROCFREQMAX1">

					<!--DC Fmax = 1500-->
					<DcMaxValue> 1500 </DcMaxValue>
				</Setting>

			</Profile>

			<!--EcoQoS profile-->
			<Profile name="Profile_EcoQos">

				<!--EPP class 0-->
				<Setting name="PERFEPP">

					<!--EPP >= LowQoS EPP-->
					<DcMinDistanceToProfile>
						<Profile> Profile_LowQos </Profile>
						<Distance> 0 </Distance>
					</DcMinDistanceToProfile>

				</Setting>

				<!--EPP class 1-->
				<Setting name="PERFEPP1">

					<!--EPP >= LowQoS EPP-->
					<DcMinDistanceToProfile>
						<Profile> Profile_LowQos </Profile>
						<Distance> 0 </Distance>
					</DcMinDistanceToProfile>

				</Setting>

				<!--Frequency Cap-->
				<Setting name="PROCFREQMAX">

					<!--DC Fmax = 1500 Mhz-->
					<DcMaxValue> 1500 </DcMaxValue>

				</Setting>

				<!--Frequency Cap1-->
				<Setting name="PROCFREQMAX1">

					<!--DC Fmax = 1500-->
					<DcMaxValue> 1500 </DcMaxValue>
				</Setting>

				<!--Setting SchedulingPolicy-->
				<Setting name="SCHEDPOLICY">
					
					<!--Prefer Big-->
					<DcValue> 2 </DcValue>
				</Setting>

				<!--Setting ShortSchedulingPolicy-->
				<Setting name="SHORTSCHEDPOLICY">

					<!--Prefer Big-->
					<DcValue> 2 </DcValue>
				</Setting>

			</Profile>

			<!--UtilityQoS profile-->
			<Profile name="Profile_UtilityQos">

				<!--EPP class 0-->
				<Setting name="PERFEPP">

					<!--EPP >= EcoQoS EPP-->
					<DcMinDistanceToProfile>
						<Profile> Profile_EcoQos </Profile>
						<Distance> 0 </Distance>
					</DcMinDistanceToProfile>
				</Setting>

				<!--EPP class 1-->
				<Setting name="PERFEPP1">

					<!--EPP >= EcoQoS EPP-->
					<DcMinDistanceToProfile>
						<Profile> Profile_EcoQos </Profile>
						<Distance> 0 </Distance>
					</DcMinDistanceToProfile>
				</Setting>

				<!--Frequency Cap-->
				<Setting name="PROCFREQMAX">

					<!--DC Fmax = 1500 Mhz-->
					<DcMaxValue> 1500 </DcMaxValue>
				</Setting>

				<!--Frequency Cap1-->
				<Setting name="PROCFREQMAX1">

					<!--DC Fmax = 1500-->
					<DcMaxValue> 1500 </DcMaxValue>
				</Setting>

				<!--Setting SchedulingPolicy-->
				<Setting name="SCHEDPOLICY">

					<!--Prefer Big-->
					<DcValue> 2 </DcValue>
				</Setting>

				<!--Setting ShortSchedulingPolicy-->
				<Setting name="SHORTSCHEDPOLICY">

					<!--Prefer Big-->
					<DcValue> 2 </DcValue>
				</Setting>

			</Profile>

			<!--MMQOS profile-->
			<Profile name ="Profile_MultimediaQos">

				<!--Setting EPP class0-->
				<Setting name="PERFEPP">

					<!--EPP = 100-->
					<DcValue> 100 </DcValue>
				</Setting>

				<!--Setting EPP class1-->
				<Setting name="PERFEPP1">

					<!--EPP = 100-->
					<DcValue> 100 </DcValue>
				</Setting>

				<!--Setting SchedulingPolicy-->
				<Setting name="SCHEDPOLICY">

					<!--Prefer Big-->
					<DcValue> 2 </DcValue>
				</Setting>

				<!--Setting ShortSchedulingPolicy-->
				<Setting name="SHORTSCHEDPOLICY">

					<!--Prefer Big-->
					<DcValue> 2 </DcValue>
				</Setting>

			</Profile>

		</Overlay>
		
	</TargetCPU>

</PpmValidationRules>