<validationRules>
	
	<!--Default overlay power scheme-->
	<overlay>
		<name> Overlay_Default </name>
		
		<!--Default profile-->
		<profile>
			<name> Profile_Default </name>
			
			<!--Setting EPP class0-->
			<setting>
				<name> PERFEPP </name>
				
				<!--Min AC value-->
				<acMinValue> 33 </acMinValue>

				<!--Min DC value-->
				<dcMinValue> 60 </dcMinValue>
				
			</setting>

			<!--Setting EPP class1-->
			<setting>
				<name> PERFEPP1 </name>

				<!--Min AC value-->
				<acMinValue> 33 </acMinValue>

				<!--Min DC value-->
				<dcMinValue> 60 </dcMinValue>

			</setting>

			<!--Setting SoftParkLatency-->
			<setting>
				<name> SOFTPARKLATENCY </name>
				
				<!-- Value = 0-->
				<dcValue> 0 </dcValue>
			</setting>

			<!--Setting SmtUnparkPolicy-->
			<setting>
				<name> SMTUNPARKPOLICY </name>

				<!-- Value = 3-->
				<dcValue> 3 </dcValue>
			</setting>

			<!--Setting ModuleUnparkPolicy-->
			<setting>
				<name> MODULEUNPARKPOLICY </name>
				
				<!-- Value = 2-->
				<dcValue> 2 </dcValue>
				
			</setting>
		</profile>

		<!--MedQoS profile-->
		<profile>
			<name> Profile_MediumQos </name>

			<!--Setting EPP class0-->
			<setting>
				<name> PERFEPP </name>

				<!--Distance to the default profile >= 20-->
				<dcMinDistanceToProfile>
					<profile> Profile_Default </profile>
					<distance> 10 </distance>
				</dcMinDistanceToProfile>

			</setting>

			<!--Setting EPP class1-->
			<setting>
				<name> PERFEPP1 </name>

				<!--Distance to the default profile >= 20-->
				<dcMinDistanceToProfile>
					<profile> Profile_Default </profile>
					<distance> 10 </distance>
				</dcMinDistanceToProfile>

			</setting>
		</profile>
		
		<!--LowQoS profile-->
		<profile> 
			<name> Profile_LowQos </name>
			
			<!--Setting EPP class0-->
			<setting>
				<name> PERFEPP </name>

				<!--Distance to the default profile >= 20-->
				<dcMinDistanceToProfile>
					<profile> Profile_Default </profile>
					<distance> 10 </distance>
				</dcMinDistanceToProfile>

			</setting>

			<!--Setting EPP class1-->
			<setting>
				<name> PERFEPP1 </name>

				<!--Distance to the default profile >= 20-->
				<dcMinDistanceToProfile>
					<profile> Profile_Default </profile>
					<distance> 10 </distance>
				</dcMinDistanceToProfile>

			</setting>
			
			<!--Frequency Cap-->
			<setting>
				<name> PROCFREQMAX </name>

				<!--DC Fmax = 1500-->
				<dcMaxValue> 1500 </dcMaxValue>
			</setting>

			<!--Frequency Cap1-->
			<setting>
				<name> PROCFREQMAX1 </name>

				<!--DC Fmax = 1500-->
				<dcMaxValue> 1500 </dcMaxValue>
			</setting>

		</profile>

		<!--EcoQoS profile-->
		<profile>
			<name> Profile_EcoQos </name>
			
			<!--EPP class 0-->
			<setting>
				<name> PERFEPP </name>

				<!--EPP >= LowQoS EPP-->
				<dcMinDistanceToProfile>
					<profile> Profile_LowQos </profile>
					<distance> 0 </distance>
				</dcMinDistanceToProfile>
				
			</setting>

			<!--EPP class 1-->
			<setting>
				<name> PERFEPP1 </name>

				<!--EPP >= LowQoS EPP-->
				<dcMinDistanceToProfile>
					<profile> Profile_LowQos </profile>
					<distance> 0 </distance>
				</dcMinDistanceToProfile>

			</setting>
			
			<!--Frequency Cap-->
			<setting>
				<name> PROCFREQMAX </name>
				
				<!--DC Fmax = 1500 Mhz-->
				<dcMaxValue> 1500 </dcMaxValue>
				
			</setting>

			<!--Frequency Cap1-->
			<setting>
				<name> PROCFREQMAX1 </name>

				<!--DC Fmax = 1500-->
				<dcMaxValue> 1500 </dcMaxValue>
			</setting>

			<!--Setting SchedulingPolicy-->
			<setting>
				<name> SCHEDPOLICY </name>

				<!--Prefer Big-->
				<dcValue> 2 </dcValue>
			</setting>

			<!--Setting ShortSchedulingPolicy-->
			<setting>
				<name> SHORTSCHEDPOLICY </name>

				<!--Prefer Big-->
				<dcValue> 2 </dcValue>
			</setting>
			
		</profile>

		<!--UtilityQoS profile-->
		<profile>
			<name> Profile_UtilityQos </name>
			
			<!--EPP class 0-->
			<setting>
				<name> PERFEPP </name>

				<!--EPP >= EcoQoS EPP-->
				<dcMinDistanceToProfile>
					<profile> Profile_EcoQos </profile>
					<distance> 0 </distance>
				</dcMinDistanceToProfile>
			</setting>

			<!--EPP class 1-->
			<setting>
				<name> PERFEPP1 </name>

				<!--EPP >= EcoQoS EPP-->
				<dcMinDistanceToProfile>
					<profile> Profile_EcoQos </profile>
					<distance> 0 </distance>
				</dcMinDistanceToProfile>
			</setting>

			<!--Frequency Cap-->
			<setting>
				<name> PROCFREQMAX </name>
				
				<!--DC Fmax = 1500 Mhz-->
				<dcMaxValue> 1500 </dcMaxValue>
			</setting>

			<!--Frequency Cap1-->
			<setting>
				<name> PROCFREQMAX1 </name>

				<!--DC Fmax = 1500-->
				<dcMaxValue> 1500 </dcMaxValue>
			</setting>

			<!--Setting SchedulingPolicy-->
			<setting>
				<name> SCHEDPOLICY </name>

				<!--Prefer Big-->
				<dcValue> 2 </dcValue>
			</setting>

			<!--Setting ShortSchedulingPolicy-->
			<setting>
				<name> SHORTSCHEDPOLICY </name>

				<!--Prefer Big-->
				<dcValue> 2 </dcValue>
			</setting>

		</profile>

		<!--MMQOS profile-->
		<profile>
			<name> Profile_MultimediaQos </name>

			<!--Setting EPP class0-->
			<setting>
				<name> PERFEPP </name>

				<!--EPP = 100-->
				<dcValue> 100 </dcValue>
			</setting>

			<!--Setting EPP class1-->
			<setting>
				<name> PERFEPP1 </name>

				<!--EPP = 100-->
				<dcValue> 100 </dcValue>
			</setting>
			
			<!--Setting SchedulingPolicy-->
			<setting>
				<name> SCHEDPOLICY </name>

				<!--Prefer Big-->
				<dcValue> 2 </dcValue>
			</setting>

			<!--Setting ShortSchedulingPolicy-->
			<setting>
				<name> SHORTSCHEDPOLICY </name>

				<!--Prefer Big-->
				<dcValue> 2 </dcValue>
			</setting>
			
		</profile>

	</overlay>

</validationRules>